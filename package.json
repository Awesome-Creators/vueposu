{
  "name": "vueposu",
  "version": "0.3.5",
  "description": "A hooks library based on Vue Composition-API",
  "repository": "git@github.com:Awesome-Creators/vueposu.git",
  "homepage": "https://vueposu.now.sh",
  "license": "MIT",
  "main": "./index.js",
  "modules": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "gen:index": "node ./scripts/imports.js",
    "build": "rimraf dist && yarn gen:index && tsc --build tsconfig.build.json && yarn format:dist && cp -R src/types dist/types && cp package.json dist && cp README.md dist",
    "format": "prettier --write \"src/**/*.{ts,vue,tsx}\" \"test/**/*.{ts,vue,tsx}\" --vue-indent-script-and-styl",
    "format:dist": "prettier --write \"dist/**/*.{js,d.ts}\"",
    "test": "jest",
    "prerelease": "yarn test",
    "release": "./scripts/release.sh",
    "test:ci": "jest --coverage --collectCoverage=true --verbose --collectCoverageFrom='<rootDir>/src/**/*.{js,ts}'",
    "test:ci-open": "yarn test:ci && open ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "mathjs": "^7.2.0",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/jest": "^26.0.10",
    "@types/lodash-es": "^4.17.3",
    "@types/mathjs": "^6.0.5",
    "@vue/compiler-sfc": "^3.0.0-rc.7",
    "@vue/test-utils": "^2.0.0-beta.3",
    "babel-core": "^7.0.0-bridge.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.11",
    "open": "^7.2.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2",
    "vue": "^3",
    "whatwg-fetch": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "yarn format"
    ]
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.1",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "keywords": [
    "vueposu",
    "vue",
    "hook",
    "hooks",
    "use",
    "usehook",
    "usehooks",
    "use-hook",
    "use-hooks",
    "vue use",
    "vue-use",
    "vue hook",
    "vue-hook",
    "vue hooks",
    "vue-hooks",
    "composition api",
    "composition-api",
    "vue-composition-api",
    "vue composition api",
    "swr",
    "vue swr",
    "vue-swr"
  ]
}
