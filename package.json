{
  "name": "euv",
  "version": "1.0.0",
  "repository": "git@github.com:Awesome-Creators/euv.git",
  "author": "yuxino <nbsawz@gmail.com>",
  "license": "MIT",
  "types": "index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.build.json && yarn format:dist && cp package.json dist && cp README.md dist",
    "format": "prettier --write \"src/**/*.{ts,vue,tsx}\" \"test/**/*.{ts,vue,tsx}\" --vue-indent-script-and-styl",
    "format:dist": "prettier --write \"dist/**/*.{js,d.ts}\"",
    "test": "jest",
    "test:ci": "jest --coverage --collectCoverage=true --verbose --collectCoverageFrom='<rootDir>/src/**/*.{ts}'"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "@types/mathjs": "^6.0.5",
    "lodash-es": "^4.17.15",
    "mathjs": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/jest": "^26.0.10",
    "@vue/compiler-sfc": "^3.0.0-rc.7",
    "@vue/test-utils": "^2.0.0-beta.3",
    "babel-core": "^7.0.0-bridge.0",
    "fast-deep-equal": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2",
    "vitepress": "^0.5.0",
    "vue": "^3.0.0-rc.7",
    "whatwg-fetch": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "yarn format"
    ]
  }
}
